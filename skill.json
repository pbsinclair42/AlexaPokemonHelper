{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Ask Alexa questions about pokemon",
          "examplePhrases": [
            "Alexa open pokemon helper",
            "Alexa ask pokemon helper what types Charizard is weak to",
            "Alexa ask pokemon helper what types Charizard resists"
          ],
          "keywords": [
            "space",
            "facts",
            "trivia"
          ],
          "name": "pokemon-helper",
          "description": "Ask Alexa questions about pokemon"
        }
      },
      "category": "KNOWLEDGE_AND_TRIVIA"
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/py/lambda_upload",
          "uri": "ask-custom-pokemon-helper-default"
        }
      }
    },
    "manifestVersion": "1.0"
  }
}
